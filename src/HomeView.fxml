<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="793.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HomeViewController">
  <content>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="1200.0" prefWidth="1181.0">
         <children>
            <!--search window-->
            <TextField fx:id="search" layoutX="672.0" layoutY="51.0" onAction="#searchWeb" prefHeight="27.0" prefWidth="302.0" promptText="Let's Googling!" />
      
                  <!--task input field-->
            <Button fx:id="ok" layoutX="1051.0" layoutY="113.0" mnemonicParsing="false" onAction="#addNewTodo" text="OK!" />
            <JFXTextField fx:id="todo" layoutX="735.0" layoutY="113.0" onAction="#addNewTodo" prefHeight="27.0" prefWidth="232.0" promptText="To Do" />
      
                  <!--todolist with timer-->
            <JFXTreeTableView fx:id="todoListTable" layoutX="495.0" layoutY="153.0" prefHeight="321.0" prefWidth="656.0" />
            <JFXComboBox fx:id="subject" layoutX="495.0" layoutY="113.0" onAction="#setCategoryOption" prefHeight="27.0" prefWidth="109.0" promptText="Subject" />
            <JFXComboBox fx:id="category" layoutX="614.0" layoutY="113.0" prefHeight="27.0" prefWidth="102.0" promptText="Category" />
         </children>
      </AnchorPane>
  </content>
</ScrollPane>
